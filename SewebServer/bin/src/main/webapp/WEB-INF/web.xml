xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
  <servlet>
    <display-name>Sample Server Application for SEWEB project</display-name>
    <servlet-name>SewebServlet</servlet-name>
    <servlet-class>fi.seweb.server.Application</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>SewebServlet</servlet-name>
    <url-pattern>/Seweb</url-pattern>
  </servlet-mapping>
</web-app>